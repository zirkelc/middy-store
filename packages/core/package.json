{
	"name": "middy-store",
	"version": "0.1.1",
	"description": "Middleware for Lambda that automatically stores and loads payloads from and to a Store like Amazon S3 or others",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"module": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./internal": "./src/internal.ts"
	},
	"files": ["dist"],
	"publishConfig": {
		"access": "public",
		"module": "./dist/index.js",
		"exports": {
			".": {
				"require": "./dist/index.cjs",
				"import": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"prepack": "cp ../../README.md ./README.md",
		"test": "vitest",
		"build": "tsup && pnpm pack | tail -n1 | xargs attw --profile node16"
	},
	"keywords": [],
	"author": "Chris Cook",
	"homepage": "https://github.com/zirkelc/middy-store",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zirkelc/middy-store.git",
		"directory": "packages/core"
	},
	"license": "MIT",
	"dependencies": {
		"lodash.get": "^4.4.2",
		"lodash.set": "^4.3.2",
		"lodash.topath": "^4.5.2",
		"ts-essentials": "^10.0.3"
	},
	"devDependencies": {
		"@middy/core": "^5.2.3",
		"@types/lodash.get": "^4.4.9",
		"@types/lodash.set": "^4.3.9",
		"@types/lodash.topath": "^4.5.9"
	},
	"peerDependencies": {
		"@middy/core": ">= 5.0.0"
	}
}
