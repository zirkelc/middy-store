{
	"name": "middy-store-s3",
	"version": "0.1.0",
	"description": "Amazon S3 Store for middy-store",
	"engines": {
		"node": ">=18"
	},
	"type": "module",
	"module": "./src/index.ts",
	"exports": {
		".": "./src/index.ts"
	},
	"files": ["dist"],
	"publishConfig": {
		"access": "public",
		"module": "./dist/index.js",
		"exports": {
			".": {
				"require": "./dist/index.cjs",
				"import": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"build": "tsup && pnpm pack | tail -n1 | xargs attw --profile node16",
		"test": "vitest"
	},
	"keywords": [],
	"author": "Chris Cook",
	"homepage": "https://github.com/zirkelc/middy-store",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zirkelc/middy-store.git",
		"directory": "packages/store-s3"
	},
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.507.0",
		"@aws-sdk/s3-request-presigner": "^3.848.0",
		"amazon-s3-url": "^1.0.0",
		"middy-store": "workspace:*"
	},
	"devDependencies": {
		"@middy/core": "^5.2.3",
		"@smithy/types": "^2.10.1"
	}
}
