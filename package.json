{
	"name": "middy-store-root",
	"private": true,
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"module": "src/index.ts",
	"exports": {
		".": {
			"import": {
				"default": "./src/index.js",
				"types": "./src/index.d.ts"
			},
			"require": {
				"default": "./src/index.js",
				"types": "./src/index.d.ts"
			}
		}
	},
	"scripts": {
		"test:unit": "vitest run --project unit",
		"test:e2e": "vitest run --project e2e",
		"test:unit:cov": "vitest run --project unit --coverage.enabled --coverage.reporter=v8 --coverage.all",
		"test:e2e:cov": "vitest run --project e2e --coverage.enabled --coverage.reporter=v8 --coverage.all",
		"build": "pnpm -r --no-bail build",
		"lint": "biome check . --write --no-errors-on-unmatched",
		"prepare": "husky",
		"release:patch": "pnpm -r release:patch",
		"release:alpha": "pnpm -r release:alpha"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@middy/core": "^5.4.3",
		"lodash.get": "^4.4.2",
		"lodash.set": "^4.3.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@tsconfig/node20": "latest",
		"@types/aws-lambda": "^8.10.140",
		"@types/lodash.get": "^4.4.9",
		"@types/lodash.set": "^4.3.9",
		"@types/node": "^20.14.10",
		"@vitest/coverage-v8": "^2.0.3",
		"@vitest/ui": "^2.0.3",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"tsup": "^8.1.0",
		"tsx": "^4.16.2",
		"typescript": "^5.5.3",
		"vitest": "^2.0.3"
	},
	"lint-staged": {
		"*.{ts,json}": ["biome check --apply --no-errors-on-unmatched"]
	}
}
